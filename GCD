#include <stdio.h>

int gcd(int a, int b) {
    while (b != 0) {
        int t = b;
        b = a % b;
        a = t;
    }
    return a;
}

int main() {
    int n;
    scanf("%d", &n);
    int a, g;
    scanf("%d", &g);
    for (int i = 1; i < n; i++) {
        scanf("%d", &a);
        g = gcd(g, a);
    }
    printf("%d\n", g);
    return 0;
}
